{
  "name": "tutor-backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.11.1",
    "npm": "6.14.5"
  },
  "scripts": {
    "start": "node ./bin/www",
    "backend": "nodemon app.js",
    "frontend": "npm run start --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongodb": "^3.5.9",
    "morgan": "~1.9.1"
  }
}
